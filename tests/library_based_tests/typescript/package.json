{
  "name": "janus-library-tests-typescript",
  "version": "1.0.0",
  "description": "TypeScript library-based tests for Janus cross-platform validation",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest --forceExit",
    "build": "tsc"
  },
  "dependencies": {
    "typescript-janus": "file:../../../TypeScriptJanus",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/uuid": "^9.0.8",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "tests/**/*.ts",
      "!tests/**/*.d.ts"
    ]
  }
}
